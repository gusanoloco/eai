<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/oxm http://www.eclipse.org/eclipselink/xsds/eclipselink_oxm_2_3.xsd" package-name="com.dev.oxm.domain"
	version="2.3">


	<java-types>
		<java-type name="Request" xml-accessor-type="NONE">
			<xml-root-element name="Request" />
			<java-attributes>
				<xml-attribute java-attribute="quote" type="Quote" />
			</java-attributes>
		</java-type>

		<java-type name="Quote" xml-accessor-type="NONE">
			<xml-root-element name="Quote" />
			<java-attributes>
				<xml-attribute java-attribute="symbol" />
				<xml-attribute java-attribute="quantity" />
				<!-- <xml-element java-attribute="buySellIndicator" /> -->
				<xml-attribute java-attribute="date" type="java.util.Date" />
			</java-attributes>
		</java-type>

	</java-types>


</xml-bindings>